{
    "name": "Kalzate",
    "main": "electron.js",
    "description": "Kalzate Server",
    "version": "0.0.1",
    "private": true,
    "dependencies": {

        "express": "3.x",
        "passport": "*",
        "passport-local": "*",
        "mongoose":"4.4.10",
        "json2csv":"*",
        "pdfkit":"*",
        "qrcode":"*",
        "barc":"*",
        "moment":"*",
        "nodemailer":"*",
        "archiver":"*",
        "electron-printer": "*"
    },
    "devDependencies": {
      "electron-packager": "^5.0.2",
      "electron-prebuilt": "^0.36.1",
      "grunt": "~0.4.1",
      "grunt-angular-templates": "~0.4.7",
      "grunt-contrib-concat": "~0.1.3",
      "grunt-contrib-jshint": "~0.4.0",
      "grunt-contrib-uglify": "~0.2.0"
    },
    "scripts": {
      "electron": "electron .",
      "db": "mongod --config /usr/local/etc/mongod.conf --dbpath ./dbpath",
      "build": "electron-packager . kalzate --platform=win32 --arch=ia32 --version=0.36.1 --ignore='electron-printer|node_modules/(electron-packager|electron-prebuilt)' --prune --out=../dist && cp -r electron-printer ../dist/kalzate-win32-ia32/resources/app/node_modules/electron-printer"
    }
}
